{
    "name": "upabase/upabase",
    "description": "A lightweight open source version of Supabase",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "slim/slim": "^4.11",
        "slim/psr7": "^1.6",
        "firebase/php-jwt": "^6.8",
        "league/flysystem": "^3.15",
        "cboden/ratchet": "^0.4",
        "evenement/evenement": "^3.0",
        "php-di/php-di": "^7.0",
        "vlucas/phpdotenv": "^5.5",
        "illuminate/database": "^10.0",
        "guzzlehttp/guzzle": "^7.0",
        "monolog/monolog": "^3.0",
        "google/apiclient": "^2.0",
        "php-curl-class/php-curl-class": "^11.0",
        "slim/php-view": "^3.3",
        "bryanjhv/slim-session": "^4.1",
        "ext-redis": "*",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": {
            "Litesupabase\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "platform": {
            "php": "8.1"
        }
    }
}
